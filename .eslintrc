{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "env": {
    "jest": true
  },
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console":0,
    "react/prop-types":0,
    "react/destructuring-assignment":0,
    "no-underscore-dangle":0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js"]
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extentions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}

